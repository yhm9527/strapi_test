{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 50,
      "maxLength": 100
    },
    "cover": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "view": {
      "default": 0,
      "type": "integer"
    },
    "like": {
      "default": 0,
      "type": "integer"
    },
    "labels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::label.label",
      "mappedBy": "posts"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "posts"
    }
  }
}
